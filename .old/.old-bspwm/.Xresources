! Font
*font:     xft:Inconsolata:size=10
*boldFont: xft:Inconsolata:size=10

! Make minimal
*internalBorder: 14
*scrollBar: false

! Colors

! Misc
*foreground:         #dedfd5
*background:         #222326

*cursorColor:        #465159
*highlightColor:     #465159
*highlightTextColor: #dedfd5

! Black
*color0:             #56636d
*color8:             #6a7a86

! Red
*color1:             #b65d64
*color9:             #e3747d

! Green
*color2:             #9bb989
*color10:            #abcc97

! Yellow
*color3:             #efc97a
*color11:            #c87f66

! Blue
*color4:             #7d96ad
*color12:            #97b6d2

! Magenta
*color5:             #ab8ba9
*color13:            #8a7489

! Cyan weird
*color6:             #70b6a8
*color14:            #79c4b5

! White weird
*color7:             #b4b9c3
*color15:            #dedfd5

! Extensions
URxvt.perl-lib:        /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,clipboard,matcher,resize-font

! Paste with Control-Shift-V
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.resize-font.smaller: C-minus
URxvt.resize-font.bigger: C-equal
URxvt.resize-font.reset: C-plus
URxvt.resize-font.show: C-question
*iso14755: False

! Enable clickable links
URxvt.url-launcher:             /usr/bin/firefox
URxvt.matcher.button:      1
URxvt.matcher.pattern.1:        \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]


! Rofi
rofi.font: System San Francisco Display 14
rofi.width: 40
rofi.padding: 18
rofi.opacity: 100
rofi.hide-scrollbar: true
rofi.eh: 1
rofi.lines: 7
rofi.color-enabled: true
rofi.color-window: #232830, #232830, #232830
rofi.color-normal: #232830, #6d7d8a, #232830, #3b454f, #e4e6db
rofi.color-active: #232830, #9bb989, #232830, #3b454f, #abcc97
rofi.color-urgent: #232830, #b65d64, #232830, #232830, #b65d64
