# This is Git's per-user configuration file.
[core]
	excludesfile = /HOMEDIR/.gitignore_global # Change
[filter "lfs"]
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
    required = true
    clean = git-lfs clean -- %f
[filter "hawser"]
  clean = git hawser clean %f
  smudge = git hawser smudge %f
  required = true
[alias]
  ck = checkout
  puu = push -u origin head
  st = status -s
  co = commit -m
  up = pull --rebase
  malina = push
  br = branch
  df = diff
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr by %an)%Creset' --abbrev-commit --date=relative
  wc = whatchanged -p --abbrev-commit --pretty=medium
  mg = merge --no-ff
[help]
  autocorrect = 1
[format]
  pretty = %C(yellow)%H%Creset -%C(green)%d%Creset %s %C(red)(%an, %cr)%Creset
[user]
  name =  # Change
  email = # Change
[color]
  ui = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[credential]
    helper = cache --timeout=2628000